            <select class="entry-input" id=brandname>
                <option value="-" id="listbrandname">BRAND NAME</option>
            </select>
        </div>

        <div class="entry-area">
            <select class="entry-input" id=itemtype>
                <option value="-" id="lisitemtype">ITEM TYPE</option>
            </select>
        </div>

        <div class="entry-area">
            <input type="text" class="entry-input" id="minstocks" name="minstocks" placeholder="MINIMUM STOCKS">
        </div>

        <div class="entry-area">
            <input type="text" class="entry-input" id="maxstocks" name="maxstocks" placeholder="MAXIMUM STOCKS">
