                'info': false,
                'scrollY': 400,
                'scrollCollapse': true,
                'serverMethod': 'GET',
                'ajax': {
                    'url': '/cablingitemmaster/getmincount'
                },
                'columns': [
                    { data: 'brandname' },
                    { data: 'itemtype' },
                    { data: 'itemcount' },
                    { data: 'minstocks' },
