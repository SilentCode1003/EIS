                    { data: 'detailid' },
                    { data: 'date' },
                    { data: 'personel' },
                    { data: 'details' },
                    { data: 'locationfrom' },
                    { data: 'locationto' },
                    { data: 'remarks' },
                    { data: 'status' },
                    { data: 'action' },
                ],
                initComplete: function () {
                    console.log('init complete');
                    hideload();
                }
            });
            //#endregion

            //#region CABLING EQUIPMENT REQUEST
