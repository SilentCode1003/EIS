                            finalData.push({
                                detailid: item.detailid,
                                date: item.prepareddate,
                                personel: item.preparedby,
                                details: details,
                                locationfrom: item.locationfrom,
                                locationto: item.locationto,
                                remarks: item.remarks,
                                status: item.status,
