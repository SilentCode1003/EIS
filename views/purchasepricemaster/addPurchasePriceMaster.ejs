<div class="tab">
    <div class="entry-area">
        <button class="autorize-btn tablinks" id="cablingBtn" name="cablingBtn"><i
                class="fas fa-fw fa-box-open"></i>CABLING</button>
        <button class="autorize-btn tablinks" id="cyberpowerBtn" name="cyberpowerBtn"><i
                class="fas fa-fw fa-box"></i>CYBERPOWER</button>
    </div>
</div>

<div class="tabcontent" id="cablingtab">
    <div class="row">
        <div class="col-xl-6 col-lg-5">
            <div class="entry-area">
                <select class="entry-input" id=brandname>
                    <option value="-" id="listbrandname">BRAND</option>
                </select>
            </div>
            <div class="entry-area">
                <select class="entry-input" id=itemtype>
                    <option value="-" id="listitemtype">TYPE</option>
                </select>
            </div>
            <div class="entry-area">
                <input type="text" class="entry-input" id="price" name="price" placeholder="PRICE">
            </div>

            <div class="entry-area">
                <button class="" id="saveBtn" name="saveBtn">SAVE</button>
            </div>
        </div>
    </div>
    <hr>
    <div>
        <table id="cabling-price-master-table">
            <thead>
                <th>ID</th>
                <th>BRAND</th>
                <th>TYPE</th>
                <th>CURRENT PRICE</th>
                <th>PREVIOUS PRICE</th>
                <th>UPDATE BY</th>
                <th>UPDATE DATE</th>
                <th>CREATED BY</th>
                <th>CREATED DATE</th>
                <th>STATUS</th>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>

<div class="tabcontent" id="cyberpowertab">
    <div class="row">
        <div class="col-xl-6 col-lg-5">
            <div class="entry-area">
                <select class="entry-input" id=cyberbrandname>
                    <option value="-" id="listcyberbrandname">BRAND</option>
                </select>
            </div>
            <div class="entry-area">
                <select class="entry-input" id=cyberitemtype>
                    <option value="-" id="listcyberitemtype">TYPE</option>
                </select>
            </div>
            <div class="entry-area">
                <input type="text" class="entry-input" id="cyberprice" name="cyberprice" placeholder="PRICE">
            </div>

            <div class="entry-area">
                <button class="" id="saveCyberBtn" name="saveCyberBtn">SAVE</button>
            </div>
        </div>
    </div>
    <hr>
    <div>
        <table id="cyberpower-price-master-table">
            <thead>
                <th>ID</th>
                <th>BRAND</th>
                <th>TYPE</th>
                <th>CURRENT PRICE</th>
                <th>PREVIOUS PRICE</th>
                <th>UPDATE BY</th>
                <th>UPDATE DATE</th>
                <th>CREATED BY</th>
                <th>CREATED DATE</th>
                <th>STATUS</th>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>