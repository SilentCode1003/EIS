    <ul class="navbar-nav sidebar sidebar sidebar-dark accordion" id="accordionSidebar">
      <%- include('sidebar.ejs') %>
    </ul>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

